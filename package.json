{
  "name": "cooker-mock",
  "version": "1.4.3",
  "description": "The best front-end local mocking tool ever!",
  "main": "index.js",
  "repository": "git@github.com:cooker-mock/cooker-mock.git",
  "author": "boyuan <531237512@outlook.com>",
  "license": "MIT",
  "bin": {
    "cooker": "index.js"
  },
  "keywords": [
    "mocking",
    "frontend",
    "local",
    "tool",
    "api",
    "development"
  ],
  "dependencies": {
    "body-parser": "^1.20.3",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "openai": "^4.87.3",
    "uuidv4": "^6.2.13",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "standard-version": "^9.5.0",
    "vitepress": "^1.6.3"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "dev:ui": "cd web-ui && yarn dev",
    "dev:both": "concurrently \"yarn dev\" \"yarn dev:ui\"",
    "build:ui": "cd web-ui && yarn build",
    "format": "prettier --write \"**/*.{js,json,css,html}\"",
    "prepare": "cd web-ui && yarn install",
    "prepublishOnly": "yarn build:ui",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  }
}
